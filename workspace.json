      }
    },
    "core.http": {
      "root": "libs/core.http",
      "sourceRoot": "libs/core.http/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core.http/tsconfig.lib.json",
              "libs/core.http/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core.http/**"]
